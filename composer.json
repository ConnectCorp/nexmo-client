{
  "name"       : "connect-corp/nexmo-client",
  "homepage" : "https://github.com/ConnectCorp/nexmo-client",
  "description": "Unofficial Nexmo Rest Client",
  "keywords"   : [
    "nexmo",
    "sdk",
    "lib"
  ],
  "license"    : "MIT",
  "type"       : "library",
  "require"    : {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload"   : {
    "psr-4": {
      "Nexmo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nexmo\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
